        </columns>
      </TableView>
      <Button layoutX="24.0" layoutY="354.0" mnemonicParsing="false" onAction="#switchToMainScene" prefHeight="25.0" prefWidth="70.0" style="-fx-background-color: #2ec0ff;" text="Back" textFill="WHITE" />
